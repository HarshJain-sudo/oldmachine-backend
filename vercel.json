{
  "version": 2,
  "builds": [
    {
      "src": "oldmachine_backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "oldmachine_backend/wsgi.py"
    }
  ],
  "functions": {
    "oldmachine_backend/wsgi.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "DJANGO_ENV": "beta"
  }
}
